{
  "name": "world-wide-map-server",
  "version": "0.1.0",
  "description": "Server for crowdsourced internet topology-map website",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "migrate:generate": "tsx -r reflect-metadata node_modules/typeorm/cli.js migration:generate -d config/data-source.ts",
    "migrate:run": "tsx -r reflect-metadata node_modules/typeorm/cli.js migration:run -d config/data-source.ts",
    "migrate:revert": "tsx -r reflect-metadata node_modules/typeorm/cli.js migration:revert -d config/data-source.ts",
    "migrate:show": "tsx -r reflect-metadata node_modules/typeorm/cli.js migration:show -d config/data-source.ts",
    "migrate": "npm run migrate:run",
    "test": "cross-env NODE_ENV=test vitest run",
    "test:watch": "cross-env NODE_ENV=test vitest",
    "test:ui": "cross-env NODE_ENV=test vitest --ui"
  },
  "keywords": [
    "express",
    "postgresql",
    "traceroute"
  ],
  "author": "matthewLacerda2",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.17",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "cross-env": "^10.1.0",
    "supertest": "^7.1.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.15"
  }
}
